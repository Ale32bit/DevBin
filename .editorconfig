[*.cs]

# CS1591: Manca il commento XML per il tipo o il membro visibile pubblicamente
dotnet_diagnostic.CS1591.severity = none

# CS8632: L'annotazione per i tipi riferimento nullable deve essere usata solo nel codice in un contesto di annotations '#nullable'.
dotnet_diagnostic.CS8632.severity = silent

# CS8618: Il campo non nullable deve contenere un valore non Null all'uscita dal costruttore. Provare a dichiararlo come nullable.
dotnet_diagnostic.CS8618.severity = silent
