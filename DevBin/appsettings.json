{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "server=localhost;uid=devbin;pwd=devbin;database=devbin;",
    "Redis": "localhost"
  },
  "Sentry": {
    "Dsn": "[DSN KEY]",
    "MaxRequestBodySize": "Always",
    "SendDefaultPii": true,
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Warning",
    "AttachStackTrace": true,
    "DiagnosticsLevel": "Error"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "IpWhitelist": [],
    "EndpointWhitelist": [],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "post:/index",
        "Period": "10s",
        "Limit": 2
      },
      {
        "Endpoint": "post:/index",
        "Period": "10m",
        "Limit": 15
      },

      {
        "Endpoint": "post:/api/*",
        "Period": "60s",
        "Limit": 5
      },
      {
        "Endpoint": "*:/api/*",
        "Period": "10s",
        "Limit": 5
      }
    ]
  },
  "PastesPath": "PastesData",
  "PasteCodeLength": 8,
  "PasteMaxSizes": {
    "Guest": 131072,
    "Member": 1048576
  },
  "LatestPastesAmount": 30,
  "SendGridAddress": "noreply@devbin.dev",
  "SendGridToken": "[TOKEN HERE]",
  "DiscordReportWebhook": "[WEBHOOK URL]",
  "AbuseIPDBToken": ""
}